package jp.terasoluna.thin.tutorial.web.transcosmos.dto;

import java.util.Date;

import org.apache.struts.upload.FormFile;

public class JNTCIEquipNoticeMgVoInput {

	private int info_id;
	private String info_caption;
	private int view_id;
	private String info_content;
	private String userId;
	private Date info_date;
	private String flag;



	public String getFlag() {
		return flag;
	}
	public void setFlag(String flag) {
		this.flag = flag;
	}

	private String info_title;
	public String getInfo_title() {
		return info_title;
	}
	public void setInfo_title(String info_title) {
		this.info_title = info_title;
	}

	//分页加
	private String totalCount = null;
	private String startIndex = null;
	private String row = null;

	private FormFile uploadFile = null;
	private String filePath = null;


	public FormFile getUploadFile() {
		return uploadFile;
	}
	public void setUploadFile(FormFile uploadFile) {
		this.uploadFile = uploadFile;
	}
	public String getFilePath() {
		return filePath;
	}
	public void setFilePath(String filePath) {
		this.filePath = filePath;
	}
	public String getTotalCount() {
		return totalCount;
	}
	public void setTotalCount(String totalCount) {
		this.totalCount = totalCount;
	}
	public String getStartIndex() {
		return startIndex;
	}
	public void setStartIndex(String startIndex) {
		this.startIndex = startIndex;
	}
	public String getRow() {
		return row;
	}
	public void setRow(String row) {
		this.row = row;
	}


	public int getInfo_id() {
		return info_id;
	}
	public void setInfo_id(int info_id) {
		this.info_id = info_id;
	}
	public int getView_id() {
		this.view_id=0;
		return view_id;
	}
	public void setView_id(int view_id) {
		this.view_id = view_id;
	}
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public Date getInfo_date() {
		return info_date;
	}
	public void setInfo_date(Date info_date) {
		this.info_date = info_date;
	}
	public String getInfo_caption() {
		return info_caption;
	}
	public void setInfo_caption(String info_caption) {
		this.info_caption = info_caption;
	}
	public String getInfo_content() {
		return info_content;
	}
	public void setInfo_content(String info_content) {
		this.info_content = info_content;
	}

}

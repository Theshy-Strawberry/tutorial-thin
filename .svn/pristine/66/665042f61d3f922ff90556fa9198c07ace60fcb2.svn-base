package jp.terasoluna.thin.tutorial.web.transcosmos.vo;

import java.io.Serializable;

/**
 * ユーザ情報保持Bean。
 *
 * データベースからの取得時、画面からの入力時に使用される。
 *
 */
public class JNTCIVocationDeptOutvo implements Serializable {

    /**
     * シリアルバージョンID。
     */
    private static final long serialVersionUID = 161610379384156463L;

    /**
     *deptname。
     */
    private String value = null;

    private String label = null;
    private String kj_name = null;
    private String cd_code = null;

    public String getKj_name() {
		return kj_name;
	}

	public void setKj_name(String kj_name) {
		this.kj_name = kj_name;
	}

	public String getCd_code() {
		return cd_code;
	}

	public void setCd_code(String cd_code) {
		this.cd_code = cd_code;
	}

	/**
     * ユーザIDを返却する。
     *
     * @return 保持するユーザID
     */
    public String getValue() {
        return value;
    }

    /**
     * ユーザIDを設定する。
     *
     * @param userId ユーザID
     */
    public void setValue(String value) {
        this.value = value;
    }

    /**
     * ユーザIDを返却する。
     *
     * @return 保持するユーザID
     */
    public String getLabel() {
        return label;
    }

    /**
     * ユーザIDを設定する。
     *
     * @param label ユーザID
     */
    public void setLabel(String label) {
        this.label = label;
    }


}

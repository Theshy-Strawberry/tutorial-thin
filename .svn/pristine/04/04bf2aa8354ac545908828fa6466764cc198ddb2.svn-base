<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE form-validation PUBLIC
          "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
          "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">

<form-validation>

	<formset>

		<form name="/JNTCIEquipListBL">
			<field property="userId" depends="numericString,maxlength,minlength">
				<arg key="JNTCIEquip02.id" position="0" />
				<arg key="${var:maxlength}" resource="false" position="1" />
				<arg key="${var:minlength}" name="minlength" resource="false"
					position="1" />
				<var>
					<var-name>maxlength</var-name>
					<var-value>4</var-value>
				</var>
				<var>
					<var-name>minlength</var-name>
					<var-value>4</var-value>
				</var>
			</field>
		</form>
	</formset>

</form-validation>